Get-WUInstall -Title "<%= scope.lookupvar('windows_updates::update_name') %>" -WhatIf -Verbose | foreach {
  if ((Test-Path "c:\ProgramData\InstalledUpdates\$($_.KB).flg") -eq $False){
    Get-WUInstall -KBArticleID "$($_.KB)" -AcceptAll
    Set-Content "c:\ProgramData\InstalledUpdates\$($_.KB).flg" "Installed"
  }
}